{
  "name": "move-elevator/me-media",
  "description": "Media center to present audio and video file as well as internal streams and stream from external provider youtube, vimeo and my video.",
  "type": "typo3-cms-extension",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "1.5.5",
    "phploc/phploc": "2.0.6",
    "phpmd/phpmd": "2.1.1",
    "sebastian/phpcpd": "2.0.1"
  },
  "config": {
    "bin-dir": "Build/bin"
  },
  "autoload": {
    "psr-4": {
      "MoveElevator\\MeMedia\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoveElevator\\MeMedia\\Tests\\": "Tests"
    }
  }
}
